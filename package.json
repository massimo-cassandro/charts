{
  "name": "@massimo-cassandro/charts",
  "version": "1.0.1",
  "description": "A collections of charts scripts",
  "main": "./dist/index.js",
  "esm": "./dist/esm/index.js",
  "author": "Massimo Cassandro",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/massimo-cassandro/charts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/charts#main"
  },
  "scripts": {
    "start": "PORT=3501 react-scripts start",
    "start (FF)": "BROWSER='Firefox' PORT=3501 react-scripts start",
    "build": "react-scripts build",
    "make-dist": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files && cp -R src/esm dist && cp -R src/js dist",
    "upd version and publish": "node ./update-version.mjs && npm publish",
    "local server (esm demo)": "python3 -m http.server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "./node_modules/@massimo-cassandro/linters-config/eslintrc.js"
    ],
    "ignorePatterns": [
      "dist/"
    ]
  },
  "stylelint": {
    "extends": "@massimo-cassandro/linters-config/stylelintrc.js",
    "ignoreFiles": [
      "dist/"
    ]
  },
  "keywords": [
    "charts",
    "javascript",
    "react"
  ],
  "browserslist": [
    "last 2 version",
    "iOS >= 13",
    "Safari >= 12",
    "not ie <= 11",
    "> 3% in IT"
  ],
  "files": [
    "dist/**/*.*"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.4",
    "@massimo-cassandro/linters-config": "^1.1.6",
    "install": "^0.13.0",
    "npm": "^8.19.2"
  }
}