  ],
  "files": [
    "src/**/*",
    "boilerplate/**/*"
  ],
  "devDependencies": {
    "@massimo-cassandro/dev-utilities": "^1.23.4",
    "@massimo-cassandro/linters-config": "^1.6.1"
