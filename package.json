{
  "name": "@massimo-cassandro/charts",
  "version": "2.1.3",
  "description": "SVG charts",
  "author": "Massimo Cassandro",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.js",
  "bugs": {
    "url": "https://github.com/massimo-cassandro/charts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/charts#main"
  },
  "scripts": {
    "python server": "python3 -m http.server 8000 # --directory __dirname__ # 8000 = default port",
    "open demo": "open http://[::1]:8000/demo/ -a 'Google Chrome'",
    "node demo": "cd demo && node node-demo.mjs",
    "upd version": "npx update-version",
    "upd@m": "npx upd@m",
    "colors css to js": "cd ./colors && node css-to-js.mjs",
    "npm-publish": "npm publish"
  },
  "keywords": [
    "charts",
    "javascript",
    "svg"
  ],
  "files": [
    "src/**/*",
    "react/**/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@massimo-cassandro/dev-updater": "^2.1.0",
    "@massimo-cassandro/eslint-config": "^2.0.6",
    "@massimo-cassandro/stylelint-config": "^1.1.9",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "stylelint": "^16.19.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-twbs-bootstrap": "^16.0.0"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.2.4",
